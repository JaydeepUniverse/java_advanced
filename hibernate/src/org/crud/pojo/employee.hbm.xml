<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

<class name="org.crud.pojo.Employee" table="EMPCRUD1">

<id column="ID" name="id" type="java.lang.Integer" >
<generator class="increment" />
</id>
<property column="name" name="name" type="java.lang.String" />
<property column="salary" name="salary" type="java.lang.Integer" />
<property column="city" name="city" type="java.lang.String" />
<property column="phone" name="phone" type="java.lang.Integer" not-null="true" />
</class>
</hibernate-mapping>